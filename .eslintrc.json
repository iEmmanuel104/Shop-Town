{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier", "standard", "prettier/standard"],
    "plugins": ["import", "prettier", "standard"],
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "new-cap": "error"
    }
}
